<% if @user == current_user %>
  <%= link_to 'Mis Viajes', '#', :class => "btn btn-block btn-sm my-button-color my-link-button isDisabled" %>
  <%= link_to 'Mis Vehiculos', user_vehicles_path(@user),:class => "btn btn-block btn-sm my-button-color my-link-button" %>
  <%= link_to 'Mis Postulaciones', '/users/'+@user.id.to_s+'/postulaciones', :class => "btn btn-block btn-sm my-button-color my-link-button" %>
  <%= link_to 'Mis Calificaciones', '#', :class => "btn btn-block btn-sm my-button-color my-link-button" %>
  <%= link_to 'Calificaciones Pendientes', '#', :class => "btn btn-block btn-sm my-button-color my-link-button" %>
  <%= link_to 'Editar Cuenta', edit_user_registration_path, :class => "btn btn-block btn-sm my-button-color my-link-button" %>
<% else %>
  <br><br>
  <%= link_to 'Viajes', '#', :class => "btn btn-block btn-sm my-button-color my-link-button isDisabled" %>
  <%= link_to 'Calificaciones', '#', :class => "btn btn-block btn-sm my-button-color my-link-button isDisabled" %>
  <%= link_to 'Vehiculos', user_vehicles_path(@user), :class => "btn btn-block btn-sm my-button-color my-link-button" %>
<% end %>
