<% if @calification.empty? %>
	No posee calificaciones pendientes.
<% else %>
	<% @calification.each do |calif| %>
	 	<div class="well my-jumbotron">>
	 		<%= link_to image_tag(calif.creador.avatar,:class=>"circular-image post-image", :alt=>'Avatar')+'  '+ usernameOf(calif.creador), user_path(calif.creador) %><br>
	 		<b>Rol: </b><br>
	 			<% if calif.tipo_calificacion == 'p' %>
	 				Piloto
	 			<% else %>
	 				Copiloto
	 			<% end %>
	 	</div>
	<% end %>
<% end %>